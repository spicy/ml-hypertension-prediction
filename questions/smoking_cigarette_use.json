{
  "SMQ020": {
    "label": "Smoked at least 100 cigarettes in life",
    "text": "These next questions are about cigarette smoking and other tobacco use. {Have you/Has SP} smoked at least 100 cigarettes in {your/his/her} entire life?",
    "target": "Both males and females 18 YEARS - 150 YEARS",
    "include": "0",
    "mappings": {
      "1": {
        "description": "Yes"
      },
      "2": {
        "description": "No",
        "skip": {
          "destination": "End of Section",
          "auto_fill": {
            "SMD030": "0",
            "SMQ040": "3",
            "SMQ050Q": "0",
            "SMQ050U": "0",
            "SMD057": "0",
            "SMD641": "0",
            "SMD650": "1",
            "SMD100FL": "2",
            "SMD100MN": "2",
            "SMQ670": "0"
          }
        }
      },
      "7": {
        "description": "Refused",
        "skip": {
          "destination": "End of Section",
          "auto_fill": {}
        }
      },
      "9": {
        "description": "Don't know",
        "skip": {
          "destination": "End of Section",
          "auto_fill": {}
        }
      }
    }
  },

  "SMD030": {
    "label": "Age started smoking cigarettes regularly",
    "text": "How old {were you/was SP} when {you/s/he} first started to smoke cigarettes fairly regularly?",
    "target": "Both males and females 18 YEARS - 150 YEARS",
    "include": "1",
    "mappings": {
      "7-76": {
        "description": "Range of Values"
      },
      "0": {
        "description": "Never smoked cigarettes regularly"
      },
      "777": {
        "description": "Refused"
      },
      "999": {
        "description": "Don't know"
      }
    }
  },

  "SMQ040": {
    "label": "Do you now smoke cigarettes?",
    "text": "{Do you/Does SP} now smoke cigarettes?",
    "target": "Both males and females 18 YEARS - 150 YEARS",
    "include": "0",
    "mappings": {
      "1": {
        "description": "Every day",
        "skip": {
          "destination": "SMD641",
          "auto_fill": {
            "SMQ050Q": "0",
            "SMQ050U": "0",
            "SMD057": "0"
          }
        }
      },
      "2": {
        "description": "Some days",
        "skip": {
          "destination": "SMD641",
          "auto_fill": {
            "SMQ050Q": "0",
            "SMQ050U": "0",
            "SMD057": "0"
          }
        }
      },
      "3": {
        "description": "Not at all"
      },
      "7": {
        "description": "Refused",
        "skip": {
          "destination": "End of Section",
          "auto_fill": {}
        }
      },
      "9": {
        "description": "Don't know",
        "skip": {
          "destination": "End of Section",
          "auto_fill": {}
        }
      }
    }
  },

  "SMQ050Q": {
    "label": "How long since quit smoking cigarettes",
    "text": "How long has it been since {you/SP} quit smoking cigarettes?",
    "target": "Both males and females 18 YEARS - 150 YEARS",
    "include": "0",
    "mappings": {
      "0": {
        "description": "Not Applicable"
      },
      "1-400": {
        "description": "Range of Values"
      },
      "66666": {
        "description": "50 or more years"
      },
      "77777": {
        "description": "Refused"
      },
      "99999": {
        "description": "Don't know"
      }
    }
  },

  "SMQ050U": {
    "label": "Unit of measure (day/week/month/year)",
    "text": "Unit of Measure",
    "target": "Both males and females 18 YEARS - 150 YEARS",
    "include": "0",
    "mappings": {
      "0": {
        "description": "Not Applicable",
        "skip": {
          "destination": "SMQQUIT",
          "auto_fill": {
            "SMQMULT": "0"
          }
        }
      },
      "1": {
        "description": "Days",
        "skip": {
          "destination": "SMQQUIT",
          "auto_fill": {
            "SMQMULT": "1"
          }
        }
      },
      "2": {
        "description": "Weeks",
        "skip": {
          "destination": "SMQQUIT",
          "auto_fill": {
            "SMQMULT": "7"
          }
        }
      },
      "3": {
        "description": "Months",
        "skip": {
          "destination": "SMQQUIT",
          "auto_fill": {
            "SMQMULT": "30"
          }
        }
      },
      "4": {
        "description": "Years",
        "skip": {
          "destination": "SMQQUIT",
          "auto_fill": {
            "SMQMULT": "365"
          }
        }
      },
      "7": {
        "description": "Refused"
      },
      "9": {
        "description": "Don't know"
      }
    }
  },

  "SMQMULT":{
    "label": "Time since quit multiplier",
    "include": "0",
    "mappings": {
      "0": {
        "description": "None"
      },
      "1": {
        "description": "Days"
      },
      "7": {
        "description": "Weeks"
      },
      "30": {
        "description": "Months"
      },
      "365": {
        "description": "Years"
      }
    },
    "formula": "1 if ##SMQ050U == 1 else ( 7 if ##SMQ050U == 2 else ( 30 if ##ALQ120U == 3 else ( 365 if ##ALQ120U == 4 else 0 ) ) )"
  },

  "SMQQUIT": {
    "label": "Days since quit smoking",
    "text": "Time in days since {you/SP} quit smoking?",
    "include": "1",
    "formula": "##ALQMULT * ##ALQ120Q * ##ALQ130"
  },

